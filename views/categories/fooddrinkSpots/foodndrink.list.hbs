<div class='spot-list wrapper'>
   {{! Content list }}
  <h2>Food and Drinks</h2>
  <br />
  <div class='list-content'>
    {{#each fooddrinkSpots}}
      <div class='each-spot'>
        <img class='img-list' src='{{imgUrl}}' alt='' />
        <div>
          <h3>{{name}}</h3>
          <hr />
          <p>{{miniDescription}}</p>
        </div>

        <form action='/fooddrinkSpots/{{_id}}/' method='get'>
          <button class='button'>See more</button>
        </form>
      </div>
    {{/each}}

  </div>

  <br />
  <a href='/index'>Return to main menu</a>

</div>